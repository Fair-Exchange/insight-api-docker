{
  "network": "livenet",
  "port": 3000,
  "services": [
    "bitcoind",
    "insight-api-safecoin",
    "insight-ui-safecoin",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/usr/src/app/.safecoin",
        "exec": "/usr/src/app/safecoin/safecoind"
      }
    },
     "insight-ui-safecoin": {
      "apiPrefix": "api"
     },
    "insight-api-safecoin": {
      "routePrefix": "api"
    }
  }
}